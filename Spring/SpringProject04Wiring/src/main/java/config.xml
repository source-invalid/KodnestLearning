<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean class="com.kodnest.spring.SpringProject04Wiring.Employee" id="emp1">
    	<property name="empId" value="13"></property>
    	<property name="empName" value="Sumit"></property>
    	<property name="empSalary" value="55000"></property>
    </bean>
    
    <bean class="com.kodnest.spring.SpringProject04Wiring.CompanyXYZ" id="cmp">
    	<property name="e" ref="emp1"></property>
    </bean>
</beans>
