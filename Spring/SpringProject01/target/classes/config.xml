<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- bean definitions here -->
    <bean class="com.kodnest.spring.SpringProject01.Student" id="st">
    	<property name="roll_no" value="11"></property>
    	<property name="Name" value="Samir"></property>
    	<property name="age" value="24"></property>
    </bean>
    
    <bean class="com.kodnest.spring.SpringProject01.JavaTeacher" id="jt">
    	<property name="teacher_id" value="12"></property>
    	<property name="teacher_name" value="Ajay"></property>
    </bean>
    
    <bean class="com.kodnest.spring.SpringProject01.SqlTeacher" id="sqt">
    	<property name="teacher_id" value="13"></property>
    	<property name="teacher_name" value="Rahul"></property>
    </bean>
    
    <bean class="com.kodnest.spring.SpringProject01.PythonTeacher" id="pt">
    	<property name="teacher_id" value="11"></property>
    	<property name="teacher_name" value="Amit"></property>
    </bean>

</beans>